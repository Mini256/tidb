DROP TABLE IF EXISTS t;
CREATE TABLE t (embedding VECTOR);
INSERT INTO t VALUES
('[1, 2, 3]'),
('[4, 5, 601]'),
('[4, 5, 61]');
SELECT
embedding,
FIRST_VALUE(embedding) OVER w AS first,
NTH_VALUE(embedding, 2) OVER w AS second,
LAST_VALUE(embedding) OVER w AS last
FROM t
WINDOW w AS (ORDER BY embedding)
ORDER BY embedding;
embedding	first	second	last
[1,2,3]	[1,2,3]	NULL	[1,2,3]
[4,5,61]	[1,2,3]	[4,5,61]	[4,5,61]
[4,5,601]	[1,2,3]	[4,5,61]	[4,5,601]
DELETE FROM t WHERE 1 = 1;
INSERT INTO t VALUES
('[1, 2, 3]'),
('[4, 5, 6]'),
('[4, 5, 6]'),
('[7, 8, 9]');
SELECT
embedding,
ROW_NUMBER() OVER w AS 'row_num',
RANK() OVER w AS 'rank',
DENSE_RANK() OVER w AS 'dense_rank'
FROM t
WINDOW w AS (ORDER BY embedding)
ORDER BY embedding;
